<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/lib/fontawesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/css/sb-admin-2.css" rel="stylesheet" />
    <link href="~/lib/datatables/media/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/lib/datatables/media/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/lib/notifyjs/dist/styles/metro/notify-metro.css" rel="stylesheet"/>
    @*<link href="~/css/site.css" rel="stylesheet">*@
</head>
<body>
    <div>
        @await Html.PartialAsync("_LoginPartial")
        <div class="row">
            <div class="col-md-1">
            </div>
            <div class="col-md-10">@RenderBody()</div>
        </div>
    </div>
    <hr />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/lib/datatables/media/js/jquery.dataTables.min.js"></script>
    <script src="~/lib/datatables/media/js/dataTables.bootstrap.min.js"></script>
    <script src="~/lib/notifyjs/dist/notify.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
